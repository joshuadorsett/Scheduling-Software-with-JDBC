<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="344.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.ConsultantReportController">
    <children>
        <TableView fx:id="consultantTable" layoutX="44.0" layoutY="117.0" onMouseClicked="#generateAppointmentTable" prefHeight="112.0" prefWidth="258.0">
            <columns>
                <TableColumn fx:id="consultantId" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="consultantName" prefWidth="182.0" text="Consultant Name" />
            </columns>
        </TableView>
        <TableView fx:id="AppointmentTable" layoutX="44.0" layoutY="270.0" prefHeight="112.0" prefWidth="258.0">
            <columns>
                <TableColumn fx:id="title" prefWidth="75.0" text="Title" />
                <TableColumn fx:id="date" prefWidth="109.0" text="Date" />
                <TableColumn fx:id="time" prefWidth="73.0" text="Time" />
            </columns>
        </TableView>
        <Label layoutX="113.0" layoutY="84.0" text="Choose a Consultant" />
        <Label layoutX="77.0" layoutY="244.0" text="Consultant's Current Appointments" />
        <Label layoutX="82.0" layoutY="24.0" text="Consultant Report">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Button fx:id="goBack" layoutX="239.0" layoutY="403.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
    </children>
</AnchorPane>
